   "source": [
    "title = \"If then else application\"\n",
    "p1, p2, p3, p4 = progs(s, \"p1 p2 p3 p4\")\n",
    "c1, c2 = sets(\"c1 c2\")\n",
    "x,y = Ints('x y')\n",
    "\n",
    "s.add(+p1, +p2, +p3, +p4)\n",
    "s.add(p1.set() == p2.set(), p2.set() == p3.set(), p3.set() == p4.set())\n",
    "s.add(p1.pre() == p2.pre(), p2.pre() == p3.pre(), p3.pre() == p4.pre())\n",
    "\n",
    "ite1 = IfThenElse(c1, p1, p2)\n",
    "ite2 = IfThenElse(c2, p3, p4)\n",
    "\n",
    "s.add(ForAll(x, c1(x) == x > 0))\n",
    "s.add(ForAll(x, c2(x) == (1 > 0)))\n",
    "s.add(ForAll([x, y], p1.post(x, y) == (x == y)))\n",
    "s.add(ForAll([x, y], p2.post(x, y) == (y == 0)))\n",
    "s.add(ForAll([x, y], p3.post(x, y) == (y == 1)))\n",
    "s.add(ForAll([x, y], p4.post(x, y) == (y == 0)))\n",
    "\n",
    "conclude(s, contracts(ite2, ite1), title)"
   ]
  }
